### Banco de Dados PostgreSQL ###
DB_TYPE=postgresdb
DB_POSTGRESDB_DATABASE=n8n_queue
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_USER=your_postgres_user
DB_POSTGRESDB_PASSWORD=your_postgres_password

### SMTP ###
N8N_SMTP_SENDER=your_email@example.com
N8N_SMTP_USER=your_email_user
N8N_SMTP_PASS=your_email_password
N8N_SMTP_HOST=smtp.yourprovider.com
N8N_SMTP_PORT=587
N8N_SMTP_SSL=false

### Criptografia ###
N8N_ENCRYPTION_KEY=your_encryption_key

### URLs ###
N8N_HOST=n8n.yourdomain.com
N8N_EDITOR_BASE_URL=https://n8n.yourdomain.com/
WEBHOOK_URL=https://n8napi.yourdomain.com/
N8N_PROTOCOL=https

### Execução ###
NODE_ENV=production
EXECUTIONS_MODE=queue
EXECUTIONS_TIMEOUT=3600
EXECUTIONS_TIMEOUT_MAX=7200
OFFLOAD_MANUAL_EXECUTIONS_TO_WORKERS=true
N8N_RUNNERS_ENABLED=true
N8N_RUNNERS_MODE=internal

### Comunidade e Pacotes ###
N8N_REINSTALL_MISSING_PACKAGES=true
N8N_COMMUNITY_PACKAGES_ENABLED=true
N8N_NODE_PATH=/home/node/.n8n/nodes
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true

### Redis ###
QUEUE_BULL_REDIS_HOST=redis
QUEUE_BULL_REDIS_PORT=6379
QUEUE_BULL_REDIS_DB=2

### AI ###
N8N_AI_ENABLED=false
N8N_AI_PROVIDER=openai
N8N_AI_OPENAI_API_KEY=

### Permissões ###
NODE_FUNCTION_ALLOW_BUILTIN=*
NODE_FUNCTION_ALLOW_EXTERNAL=moment,lodash

### Timezone ###
GENERIC_TIMEZONE=America/Sao_Paulo
TZ=America/Sao_Paulo

### Outras ###
N8N_DIAGNOSTICS_ENABLED=false
N8N_LOG_LEVEL=info
N8N_PAYLOAD_SIZE_MAX=32
N8N_METRICS=true
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=336